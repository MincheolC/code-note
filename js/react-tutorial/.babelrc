{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js"],
        "alias": {
          "_components": "./src/components"
        }
      }
    ],
    [ "@babel/plugin-proposal-class-properties" ]
  ]
}